{
  "name": "xo-summary",
  "version": "2.1.0",
  "description": "Display output from XO as a list of style errors, ordered by count",
  "type": "module",
  "bin": "index.js",
  "preferGlobal": true,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "xo && npm run test:default && npm run test:name-only",
    "test:default": "cat test/fixture.txt | node index.js",
    "test:name-only": "cat test/fixture.txt | node index.js --name-only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LitoMore/xo-summary.git"
  },
  "author": "LitoMore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LitoMore/xo-summary/issues"
  },
  "homepage": "https://github.com/LitoMore/xo-summary#readme",
  "dependencies": {
    "concat-stream": "^2.0.0",
    "pad": "^3.3.0",
    "strip-ansi": "^7.1.0"
  },
  "devDependencies": {
    "xo": "^1.2.1"
  }
}
